{
    "name": "jasonloeve/nhi_field",
    "description": "Provide a NHI db field and NHI Form field to track NZ's \"National Health Index\" number.",
    "type": "silverstripe-vendormodule",
    "license": "MIT",
    "authors": [
        {
            "name": "Maxime Rainville",
            "email": "maxime@webtorque.co.nz"
        },
        {
            "name": "Jason Loeve",
            "email": "jason@weareonfire.co.nz"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
